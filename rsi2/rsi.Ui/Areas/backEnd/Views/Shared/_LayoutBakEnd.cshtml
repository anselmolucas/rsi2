<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>RSI - BackEnd</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="icon" type="image/ico" href="~/assets/images/logos/favicon.png">

    <!-- styles AdminLTE - theme-->
    <link href="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/plugins/ionicons/css/ionicons.min.css" rel="stylesheet" />
    <link href="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/dist/css/admin-lte.min.css" rel="stylesheet" />
    <link href="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/dist/css/skins/_all-skins.min.css" rel="stylesheet" />

    <!-- icons-->
    <link href="~/assets/stylesAndScripts/icons/flaticon/flaticon.css" rel="stylesheet" />
    <link href="~/Areas/backEnd/assets/stylesAndScripts/fonts/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />

    <link href="~/Areas/backEnd/assets/stylesAndScripts/this/this.css" rel="stylesheet" />
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        @Html.Action("NavBar", "PageElements_backEnd", new { __cursoId = 1, __studentId = 1 })

        @Html.Action("SideBarLeft", "PageElements_backEnd", new { __cursoId = 1, __studentId = 1 })

        <div class="content-wrapper">
            <section class="content-header">
                <h1>
                    @ViewBag.Title
                    <small>@ViewBag.SubTitle</small>
                </h1>
                <ol class="breadcrumb">
                    @if (!String.IsNullOrEmpty(ViewBag.PreviousScreenLink))
                    {
                        <li><a href="@Url.Content(ViewBag.PreviousScreenLink)"><i class="fa fa-dashboard"></i>&nbsp;&nbsp;@ViewBag.PreviousScreen</a></li>
                    }
                    else
                    {
                        <li><i class="fa fa-dashboard"></i>&nbsp;&nbsp;@ViewBag.PreviousScreen</li>
                    }
                    <li class="active">@ViewBag.CurrentScreen</li>
                </ol>
            </section>

            <section class="content">
                @if (TempData["message"] != null)
                {
                    <div class="alert alert-success">@TempData["message"]</div>
                }
                <div class="row">
                    @if (IsSectionDefined("mailboxFoldersMenu"))
                    {
                        <div class="col-md-3">
                            @RenderSection("mailboxFoldersMenu", required: false)
                        </div>

                        <div class="col-md-9">
                            @RenderBody()
                        </div>

                    }
                    else
                    {
                        @RenderBody()
                    }
                </div>
            </section>
        </div>

        @Html.Action("Footer", "PageElements_backEnd", new { __cursoId = 1, __studentId = 1 })

        @Html.Action("SideBarRight", "PageElements_backEnd", new { __cursoId = 1, __studentId = 1 })

        <div class="control-sidebar-bg"></div>
    </div>

    <script src="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/plugins/jquery/js/jQuery-2.1.4.min.js"></script>
    @RenderSection("menu", required: false)
    <script src="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/plugins/slimscroll/js/jquery.slimscroll.min.js"></script>
    <script src="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/plugins/fastclick/js/fastclick.min.js"></script>
    <script src="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/dist/js/app.min.js"></script>
    @RenderSection("scripts", required: false)

    <!-- validação dos formulários (jquery validate)-->
    <script src="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/plugins/jquery-validate/js/jquery.validate.min.js"></script>
    <script src="~/Areas/backEnd/assets/stylesAndScripts/AdminLTE/plugins/jquery-validate/js/jquery.validate.unobtrusive.min.js"></script>
    <script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.js"></script>

    <!-- mascara de edição em formulários-->
    <script src="~/assets/stylesAndScripts/jquery.inputmask/inputmask.js"></script>
    <script src="~/assets/stylesAndScripts/jquery.inputmask/jquery.inputmask.js"></script>
    <script src="~/assets/stylesAndScripts/jquery.inputmask/inputmask.extensions.min.js"></script>
    <script src="~/assets/stylesAndScripts/jquery.inputmask/inputmask.date.extensions.min.js"></script>
    <script src="~/assets/stylesAndScripts/jquery.inputmask/inputmask.numeric.extensions.js"></script>

    <script src="~/Areas/backEnd/assets/stylesAndScripts/this/myScriptsBackEnd.js"></script>
    <script src="~/Areas/backEnd/assets/stylesAndScripts/this/this.js"></script>

</body>
</html>