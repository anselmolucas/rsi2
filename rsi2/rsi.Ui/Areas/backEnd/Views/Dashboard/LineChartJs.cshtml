@{ 
    ViewBag.Meta_jan = 0.00;
    ViewBag.Meta_fev = 0.00;
    ViewBag.Meta_mar = 0.00;
    ViewBag.Meta_abr = 0.00;
    ViewBag.Meta_mai = 0.00;
    ViewBag.Meta_jun = 2.00;
    ViewBag.Meta_jul = 5.00;
    ViewBag.Meta_ago = 3.00;
    ViewBag.Meta_set = 6.00;
    ViewBag.Meta_out = 7.00;
    ViewBag.Meta_nov = 9.00;
    ViewBag.Meta_dez = 10.00;
    double _meta_total = ViewBag.Meta_jan + ViewBag.Meta_fev + ViewBag.Meta_mar + ViewBag.Meta_abr + ViewBag.Meta_mai + ViewBag.Meta_jun + ViewBag.Meta_jul + ViewBag.Meta_ago + ViewBag.Meta_set + ViewBag.Meta_out + ViewBag.Meta_nov + ViewBag.Meta_dez;
    double _views_total = ViewBag.TotalAdvertisers_jan + ViewBag.TotalAdvertisers_fev + ViewBag.TotalAdvertisers_mar + ViewBag.TotalAdvertisers_abr + ViewBag.TotalAdvertisers_mai + ViewBag.TotalAdvertisers_jun + ViewBag.TotalAdvertisers_jul + ViewBag.TotalAdvertisers_ago + ViewBag.TotalAdvertisers_set + ViewBag.TotalAdvertisers_out + ViewBag.TotalAdvertisers_nov + ViewBag.TotalAdvertisers_dez;

    var _perc_jan = Math.Round(Convert.ToDecimal(ViewBag.Meta_jan == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_jan) / Convert.ToDecimal(ViewBag.Meta_jan))), 2);
    var _perc_fev = Math.Round(Convert.ToDecimal(ViewBag.Meta_fev == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_fev) / Convert.ToDecimal(ViewBag.Meta_fev))), 2);
    var _perc_mar = Math.Round(Convert.ToDecimal(ViewBag.Meta_mar == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_mar) / Convert.ToDecimal(ViewBag.Meta_mar))), 2);
    var _perc_abr = Math.Round(Convert.ToDecimal(ViewBag.Meta_abr == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_abr) / Convert.ToDecimal(ViewBag.Meta_abr))), 2);
    var _perc_mai = Math.Round(Convert.ToDecimal(ViewBag.Meta_mai == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_mai) / Convert.ToDecimal(ViewBag.Meta_mai))), 2);
    var _perc_jun = Math.Round(Convert.ToDecimal(ViewBag.Meta_jun == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_jun) / Convert.ToDecimal(ViewBag.Meta_jun))), 2);
    var _perc_jul = Math.Round(Convert.ToDecimal(ViewBag.Meta_jul == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_jul) / Convert.ToDecimal(ViewBag.Meta_jul))), 2);
    var _perc_ago = Math.Round(Convert.ToDecimal(ViewBag.Meta_ago == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_ago) / Convert.ToDecimal(ViewBag.Meta_ago))), 2);
    var _perc_set = Math.Round(Convert.ToDecimal(ViewBag.Meta_set == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_set) / Convert.ToDecimal(ViewBag.Meta_set))), 2);
    var _perc_out = Math.Round(Convert.ToDecimal(ViewBag.Meta_out == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_out) / Convert.ToDecimal(ViewBag.Meta_out))), 2);
    var _perc_nov = Math.Round(Convert.ToDecimal(ViewBag.Meta_nov == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_nov) / Convert.ToDecimal(ViewBag.Meta_nov))), 2);
    var _perc_dez = Math.Round(Convert.ToDecimal(ViewBag.Meta_dez == 0 ? 0 : (Convert.ToDecimal(ViewBag.TotalAdvertisers_dez) / Convert.ToDecimal(ViewBag.Meta_dez))), 2);
    var _perc_total = Math.Round(_meta_total == 0 ? 0 : _views_total / _meta_total, 2);
}
<div class="box box-info">
    <div class="box-header with-border">
        <h3 class="box-title">@ViewBag.ChartLineTitle</h3>
        <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                <i class="fa fa-minus"></i>
            </button>
            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
        </div>
    </div>
    <div class="box-body">
        <div class="chart">
            <canvas id="lineChart" style="height:250px"></canvas>
        </div>
        <br />
        <div class="panel panel-default">
            <div class="panel-body">
                <table class="table table-hover table-striped">
                    <thead>
                        <tr>
                            <th></th>
                            <th>jan</th>
                            <th>fev</th>
                            <th>mar</th>
                            <th>abr</th>
                            <th>mai</th>
                            <th>jun</th>
                            <th>jul</th>
                            <th>ago</th>
                            <th>set</th>
                            <th>out</th>
                            <th>nov</th>
                            <th>dez</th>
                            <th>tt</th>
                        </tr>
                    </thead>
                    <tbody>
                        
                        <tr>
                            <td><i class="glyphicon glyphicon-stop" style="color:rgba(60,141,188,0.9);"></i> <b>real</b></td>
                            <td>@ViewBag.TotalAdvertisers_jan</td>
                            <td>@ViewBag.TotalAdvertisers_fev</td>
                            <td>@ViewBag.TotalAdvertisers_mar</td>
                            <td>@ViewBag.TotalAdvertisers_abr</td>
                            <td>@ViewBag.TotalAdvertisers_mai</td>
                            <td>@ViewBag.TotalAdvertisers_jun</td>
                            <td>@ViewBag.TotalAdvertisers_jul</td>
                            <td>@ViewBag.TotalAdvertisers_ago</td>
                            <td>@ViewBag.TotalAdvertisers_set</td>
                            <td>@ViewBag.TotalAdvertisers_out</td>
                            <td>@ViewBag.TotalAdvertisers_nov</td>
                            <td>@ViewBag.TotalAdvertisers_dez</td>
                            <td><b>@_views_total</b></td>
                        </tr>
                        <tr>
                            <td><i class="glyphicon glyphicon-stop" style="color:rgba(210, 214, 222, 1);"></i> <b>meta</b></td>
                            <td>@ViewBag.Meta_jan</td>
                            <td>@ViewBag.Meta_fev</td>
                            <td>@ViewBag.Meta_mar</td>
                            <td>@ViewBag.Meta_abr</td>
                            <td>@ViewBag.Meta_mai</td>
                            <td>@ViewBag.Meta_jun</td>
                            <td>@ViewBag.Meta_jul</td>
                            <td>@ViewBag.Meta_ago</td>
                            <td>@ViewBag.Meta_set</td>
                            <td>@ViewBag.Meta_out</td>
                            <td>@ViewBag.Meta_nov</td>
                            <td>@ViewBag.Meta_dez</td>                            
                            <td><b>@_meta_total</b></td>
                        </tr>
                        <tr>
                            <td><b>%</b></td>
                            <td><b>@_perc_jan.ToString("0%")</b></td>
                            <td><b>@_perc_fev.ToString("0%")</b></td>
                            <td><b>@_perc_mar.ToString("0%")</b></td>
                            <td><b>@_perc_abr.ToString("0%")</b></td>
                            <td><b>@_perc_mai.ToString("0%")</b></td>
                            <td><b>@_perc_jun.ToString("0%")</b></td>
                            <td><b>@_perc_jul.ToString("0%")</b></td>
                            <td><b>@_perc_ago.ToString("0%")</b></td>
                            <td><b>@_perc_set.ToString("0%")</b></td>
                            <td><b>@_perc_out.ToString("0%")</b></td>
                            <td><b>@_perc_nov.ToString("0%")</b></td>
                            <td><b>@_perc_dez.ToString("0%")</b></td>
                            <td><b>@_perc_total.ToString("0%")</b></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>

    </div>
</div>

<script>
    $(function () {        
        //-------------
        //- LINE CHART -
        //--------------
        //Create the line chart
        var lineChartData = {
            labels: ["jan", "fev", "mar", "abr", "mai", "jun", "jul", "ago", "set", "out", "nov", "dez" ],
            datasets: [
              {
                  label: "meta",
                  fillColor: "rgba(210, 214, 222, 1)",
                  strokeColor: "rgba(210, 214, 222, 1)",
                  pointColor: "rgba(210, 214, 222, 1)",
                  pointStrokeColor: "#c1c7d1",
                  pointHighlightFill: "#fff",
                  pointHighlightStroke: "rgba(220,220,220,1)",
                  data: [@ViewBag.Meta_jan, @ViewBag.Meta_fev, @ViewBag.Meta_mar, @ViewBag.Meta_abr, @ViewBag.Meta_mai, @ViewBag.Meta_jun, @ViewBag.Meta_jul, @ViewBag.Meta_ago, @ViewBag.Meta_set, @ViewBag.Meta_out, @ViewBag.Meta_nov, @ViewBag.Meta_dez]
              },
              {
                  label: "real",
                  fillColor: "rgba(60,141,188,0.9)",
                  strokeColor: "rgba(60,141,188,0.8)",
                  pointColor: "#3b8bba",
                  pointStrokeColor: "rgba(60,141,188,1)",
                  pointHighlightFill: "#fff",
                  pointHighlightStroke: "rgba(60,141,188,1)",
                  data: [@ViewBag.TotalAdvertisers_jan, @ViewBag.TotalAdvertisers_fev, @ViewBag.TotalAdvertisers_mar, @ViewBag.TotalAdvertisers_abr, @ViewBag.TotalAdvertisers_mai, @ViewBag.TotalAdvertisers_jun, @ViewBag.TotalAdvertisers_jul, @ViewBag.TotalAdvertisers_ago, @ViewBag.TotalAdvertisers_set, @ViewBag.TotalAdvertisers_out, @ViewBag.TotalAdvertisers_nov, @ViewBag.TotalAdvertisers_dez]
              }
            ]
        };

        var lineChartOptions = {
            //Boolean - If we should show the scale at all
            showScale: true,
            //Boolean - Whether grid lines are shown across the chart
            scaleShowGridLines: true,
            //String - Colour of the grid lines
            scaleGridLineColor: "rgba(0,0,0,.05)",
            //Number - Width of the grid lines
            scaleGridLineWidth: 1,
            //Boolean - Whether to show horizontal lines (except X axis)
            scaleShowHorizontalLines: true,
            //Boolean - Whether to show vertical lines (except Y axis)
            scaleShowVerticalLines: true,
            //Boolean - Whether the line is curved between points
            bezierCurve: true,
            //Number - Tension of the bezier curve between points
            bezierCurveTension: 0.3,
            //Boolean - Whether to show a dot for each point
            pointDot: false,
            //Number - Radius of each point dot in pixels
            pointDotRadius: 4,
            //Number - Pixel width of point dot stroke
            pointDotStrokeWidth: 1,
            //Number - amount extra to add to the radius to cater for hit detection outside the drawn point
            pointHitDetectionRadius: 20,
            //Boolean - Whether to show a stroke for datasets
            datasetStroke: true,
            //Number - Pixel width of dataset stroke
            datasetStrokeWidth: 2,
            //Boolean - Whether to fill the dataset with a color
            datasetFill: true,
            //String - A legend template
            legendTemplate: "<ul class=\"<%=name.toLowerCase()%>-legend\"><% for (var i=0; i<datasets.length; i++){%><li><span style=\"background-color:<%=datasets[i].lineColor%>\"></span><%if(datasets[i].label){%><%=datasets[i].label%><%}%></li><%}%></ul>",
            //Boolean - whether to maintain the starting aspect ratio or not when responsive, if set to false, will take up entire container
            maintainAspectRatio: true,
            //Boolean - whether to make the chart responsive to window resizing
            responsive: true
        };
       
        var lineChartCanvas = $("#lineChart").get(0).getContext("2d");
        var lineChart = new Chart(lineChartCanvas);
        
        lineChartOptions.datasetFill = false;
        lineChart.Line(lineChartData, lineChartOptions);
    });
</script>
